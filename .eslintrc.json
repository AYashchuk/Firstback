{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "new-cap":"off",
        "operator-linebreak": "off",
        "arrow-parens": ["error", "as-needed"],
        "indent": "off",
        "object-curly-spacing": ["error", "always"],
        "require-jsdoc": "off",
        "linebreak-style": "off",
        "max-len": ["error", { "code": 120 }]
    }
}
